{
  "name": "vai-ts",
  "version": "4.0.4",
  "description": "Projeto criado para inicializar projetos com Typescript",
  "repository": "git@github.com:lucaslacerdacl/vai-ts.git",
  "author": "Lucas <lucaslacerdacl@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "build/src/index.js",
  "files": [
    "/build"
  ],
  "bin": {
    "vai-ts": "build/src/index.js"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "lint": "gts lint",
    "lint-clean": "gts clean",
    "lint-fix": "gts fix",
    "compile": "tsc --project tsconfig.production.json",
    "compile-watch": "yarn:compile --watch",
    "test:unit": "jest --config=jest.unit.config.js --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit:coverage": "yarn test:unit --coverage",
    "test:unit:coverage-upload": "yarn test:unit:coverage && codecov",
    "test:integration": "jest --config=jest.integration.config.js --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test:integration:watch": "yarn test:integration --watch",
    "test:integration:coverage": "yarn test:integration --coverage --no-cache --ci",
    "docs": "typedoc src/*.ts src/**/*.ts"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.11.2",
    "@types/ora": "^3.2.0",
    "codecov": "^3.8.1",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.35",
    "typescript": "^4.0.3",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "chalk": "^4.1.1",
    "inquirer": "^8.1.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13"
  }
}
