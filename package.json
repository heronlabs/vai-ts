{
  "name": "vai-ts",
  "version": "1.17.1",
  "description": "Projeto criado para inicializar projetos com Typescript",
  "repository": "git@github.com:lucaslacerdacl/vai-ts.git",
  "author": "Lucas <lucaslacerdacl@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "build/src/index.js",
  "files": [
    "/build"
  ],
  "bin": {
    "vai-ts": "build/src/index.js"
  },
  "scripts": {
    "build": "gulp build",
    "lint": "gts lint",
    "lint-clean": "gts clean",
    "lint-fix": "gts fix",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
    "test-watch": "yarn test --watch",
    "test-coverage": "yarn test --coverage",
    "test-coverage-upload": "yarn test-coverage && codecov",
    "docs": "typedoc src/*.ts src/**/*.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@types/gulp": "^4.0.8",
    "@types/gulp-shell": "^0.7.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.11.2",
    "@types/vinyl-paths": "^0.0.31",
    "babel-jest": "^26.6.3",
    "codecov": "^3.8.1",
    "del": "^6.0.0",
    "gts": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-shell": "^0.8.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.35",
    "typescript": "^4.0.3",
    "vinyl-paths": "^3.0.1",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13"
  }
}
